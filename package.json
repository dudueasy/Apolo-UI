{
  "name": "apolo-ui-test",
  "version": "0.3.23",
  "description": "A UI Components Library Built with React & TypeScript",
  "main": "dist/lib/apoloUI.js",
  "types": "dist/lib/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "generate-types": "tsc -d --emitDeclarationOnly --allowJs false",
    "build": "rm -rf dist/* && webpack --config webpack.config.prod.js && yarn generate-types",
    "start": "webpack-dev-server --config webpack.config.dev.js",
    "analyze": "webpack  --config webpack.config.prod.js --profile --json > stats.json  && webpack-bundle-analyzer stats.json dist/lib",
    "test": "jest --no-cache --watchAll",
    "test:ci": "jest --config=jest.config.ci.js",
    "deploy": "./deploy.sh patch"
  },
  "keywords": [
    "React",
    "TypeScript",
    "UI",
    "Unit Testing"
  ],
  "author": "Apolo",
  "license": "MIT",
  "devDependencies": {
    "@types/enzyme": "^3.9.1",
    "@types/jest": "^24.0.12",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.4",
    "@types/webpack-env": "^1.13.9",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "html-webpack-plugin": "^3.2.0",
    "jest-environment-enzyme": "^7.0.2",
    "jest-enzyme": "^7.0.2",
    "jest-junit": "^6.4.0",
    "node-sass": "^4.12.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^4.1.6",
    "thread-loader": "^2.1.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "@types/react-test-renderer": "^16.8.1",
    "jest": "^24.8.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "jest-enzyme",
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    }
  }
}
